(
 (
  "spacemacs" . (
                 (user-emacs-directory . "~/.spacemacs") 
                 (env . (("SPACEMACSDIR" . "~/.spacemacs-config")))
                 )
  )
 (
  ;; note that the environment variable $DOOMDIR is used when running the doom binary
  "doom" . (
            (user-emacs-directory . "~/.doomemacs")
            (env . (("DOOMDIR" . "~/.doomemacs-config")))
            )
  )
(
  "doom2" . (
            (user-emacs-directory . "~/.doomemacs")
            (env . (("DOOMDIR" . "~/.doom.d")))
            )
  )


 ("default" . ((user-emacs-directory . "~/.emacs.default")))
 ("vanilla" . ((user-emacs-directory . "~/.emacs.vanilla")))
 ("evil" . ((user-emacs-directory . "~/.emacs.evil")))
 ("god" . ((user-emacs-directory . "~/.emacs.god")))
)





